<Window x:Class="Inmobiliaria.Client.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="531" Width="922" WindowState="Maximized" WindowStartupLocation="CenterScreen">
    <Grid Name="MainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="15*" />
            <ColumnDefinition Width="70*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="10*" />
            <RowDefinition Height="70*" />
        </Grid.RowDefinitions>
        <Grid Name="gridUCContainer"  Grid.Row="1" Grid.ColumnSpan="2"></Grid>
        <StackPanel Grid.Column="1" Orientation="Horizontal">
            <Button Content="Edificios"  Height="23" Name="btnOpenEdificio" Width="80" Click="btnOpenEdificio_Click" />
            <Button Content="Casas" Height="23" Name="btn_OpenCasa" Width="80" Click="btn_OpenCasa_Click" />
            <Button Content="Buscar" Height="23" />
            <Button Content="Apartamentos" Height="23"  />
            <Button Content="Servicios" Height="23"  />
            <Button Content="Infraestructuras" Height="23" />
            
        </StackPanel>
    </Grid>
</Window>
