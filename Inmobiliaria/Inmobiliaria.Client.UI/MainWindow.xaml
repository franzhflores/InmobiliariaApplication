<Window x:Class="Inmobiliaria.Client.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="531" Width="922" WindowState="Maximized" WindowStartupLocation="CenterScreen">
    <Grid Name="MainPanel">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="15*" />
            <ColumnDefinition Width="70*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="10*" />
            <RowDefinition Height="70*" />
        </Grid.RowDefinitions>
        <Grid Name="gridUserControlContainer" Grid.Column="1" Grid.Row="1"></Grid>
        <StackPanel Grid.Column="1" Orientation="Horizontal">
            <Button Content="Open Edificio"  Height="23" Name="btnOpenEdificio" Width="97" Click="btnOpenEdificio_Click" />
            <Button Content="Open Casa" Height="23" Name="btn_OpenCasa" Width="106" Click="btn_OpenCasa_Click" />
        </StackPanel>
    </Grid>
</Window>
